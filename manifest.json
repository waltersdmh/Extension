{
  "manifest_version": 2,

  "name": "Amazon Review Filter",
  "description": "Attempt to filter Amazon reviews",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

"content_security_policy":"script-src 'self' https://code.jquery.com/jquery-3.1.1.js https://rawgit.com/bartaz/sandbox.js/master/jquery.highlight.js 'unsafe-eval'; object-src 'self'",


  "permissions": [
    "activeTab",
    "tabs", "http://*/*", "https://*/*","file:///*",
    "https://ajax.googleapis.com/"
  ]


}